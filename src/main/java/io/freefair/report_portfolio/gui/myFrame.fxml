<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.freefair.report_portfolio.gui.MyFrameController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <GridPane GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="NEVER" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" />
            <ColumnConstraints hgrow="NEVER" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" />
            <ColumnConstraints hgrow="ALWAYS" minWidth="420.0" prefWidth="420.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="30.0" prefHeight="30.0" vgrow="NEVER" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button alignment="TOP_RIGHT" mnemonicParsing="false" onAction="#next" text="Next" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER" />
            <Button mnemonicParsing="false" onAction="#previous" text="Previous" />
            <Button mnemonicParsing="false" onAction="#today" text="Today" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER">
               <GridPane.margin>
                  <Insets right="50.0" />
               </GridPane.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#save" text="Save" GridPane.columnIndex="2" GridPane.halignment="RIGHT">
               <GridPane.margin>
                  <Insets right="110.0" />
               </GridPane.margin>
            </Button>
            <TextArea id="txtContentMonday" fx:id="txtContentMonday" prefHeight="200.0" prefWidth="200.0" wrapText="true" GridPane.columnIndex="2" GridPane.rowIndex="1" />
            <TextField id="txtTimeMonday" fx:id="txtTimeMonday" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="TOP" />
            <TextField id="txtDateMonday" fx:id="txtDateMonday" GridPane.rowIndex="1" GridPane.valignment="TOP" />
            <TextField id="txtDateTuesday" fx:id="txtDateTuesday" GridPane.rowIndex="2" />
            <TextField id="txtTimeTuesday" fx:id="txtTimeTuesday" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextArea id="txtContentTuesday" fx:id="txtContentTuesday" prefHeight="200.0" prefWidth="200.0" wrapText="true" GridPane.columnIndex="2" GridPane.rowIndex="2" />
            <TextArea id="txtContentWednesday" fx:id="txtContentWednesday" prefHeight="200.0" prefWidth="200.0" wrapText="true" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <TextArea id="txtContentThursday" fx:id="txtContentThursday" prefHeight="200.0" prefWidth="200.0" wrapText="true" GridPane.columnIndex="2" GridPane.rowIndex="4" />
            <TextArea id="txtContentFriday" fx:id="txtContentFriday" prefHeight="200.0" prefWidth="200.0" wrapText="true" GridPane.columnIndex="2" GridPane.rowIndex="5" />
            <TextField id="txtTimeWednesday" fx:id="txtTimeWednesday" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField id="txtTimeThursday" fx:id="txtTimeThursday" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField id="txtTimeFriday" fx:id="txtTimeFriday" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField id="txtDateWednesday" fx:id="txtDateWednesday" GridPane.rowIndex="3" />
            <TextField id="txtDateThursday" fx:id="txtDateThursday" GridPane.rowIndex="4" />
            <TextField id="txtDateFriday" fx:id="txtDateFriday" GridPane.rowIndex="5" />
            <Button mnemonicParsing="false" onAction="#open" text="Open" GridPane.columnIndex="2" GridPane.halignment="RIGHT">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <GridPane.margin>
                  <Insets right="161.0" />
               </GridPane.margin>
            </Button>
         </children>
      </GridPane>
   </children>
</GridPane>
